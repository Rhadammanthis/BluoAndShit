<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="5dp" />
            <stroke android:width="1dip" android:color="@color/blueButtonDark" />
            <gradient android:angle="-90" android:startColor="@color/blueButtonDark" android:endColor="@color/blueButtonDark"  />
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="5dp" />
            <stroke android:width="1dip" android:color="@color/blueButtonPrimary" />
            <gradient android:angle="-90" android:startColor="@color/blueButtonPrimary" android:endColor="@color/blueButtonPrimary" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle"  >
            <corners android:radius="5dp" />
            <stroke android:width="1dip" android:color="@color/blueButtonDisabled" />
            <gradient android:angle="-90" android:startColor="@color/blueButtonDisabled" android:endColor="@color/blueButtonDisabled" />
        </shape>
    </item>
</selector>